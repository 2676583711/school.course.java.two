public class StringUtil {

	public static void main(String[] args) {
		String str = "java";
		char[] ch = str.toCharArray();
		for (int i = 0; i < ch.length; i++) {

			if (i != ch.length - 1) {
				System.out.println("char[" + i + "]" + ch[i]);

			} else {
				System.out.println(ch[i]);

			}
		}
	}
}
