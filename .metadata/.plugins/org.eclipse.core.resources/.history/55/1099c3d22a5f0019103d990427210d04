package cn.zhou.work3;

public class Example3 {

	
}


interface Shape{
	public double area(double x);
}


//求正方形的面e
class Square implements Shape{

	@Override
	public double area(double x) {
		
		return x*x;
	}
	
}

//求A形的面e
class Circle implements Shape{

	@Override
	public double area(double x) {
		return x*x*3.14;
	}
	
}