	public static void main(String[] args) {

		try {
			FileInputStream fileInputStream = new FileInputStream("a.txt");
			int b = 0;
			while ((b = fileInputStream.read()) != -1) {

			}
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
